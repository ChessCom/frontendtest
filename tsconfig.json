{
	"compilerOptions": {
		"lib": [
			"es2021",
			"dom",
			"dom.iterable"
		],
		"target": "es2021",
		"module": "NodeNext",
		"esModuleInterop": true,
		"jsx": "react",
		"jsxFactory": "h",
		"jsxFragmentFactory": "Fragment",
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"strict": true
	}
}
